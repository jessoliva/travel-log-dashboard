<!-- travel section -->
<section class="travel p-3">
  <!-- login section -->
  <div class="user-log">
    <form id="sign-up-form">
      <input
        name="name"
        id="name"
        type="text"
        autofocus="true"
        placeholder="Name"
        class="form-input p-2 rounded-pill text-center"
      />
      <input
        name="username"
        id="username"
        type="text"
        autofocus="true"
        placeholder="Username"
        class="form-input p-2 rounded-pill text-center"
      />
      <input
        name="email"
        id="email"
        type="text"
        autofocus="true"
        placeholder="Email"
        class="form-input p-2 rounded-pill text-center"
      />
      <input
        name="password"
        id="password"
        type="text"
        autofocus="true"
        placeholder="Password"
        class="form-input p-2 rounded-pill text-center"
      />
      <button
        id="search-city"
        type="submit"
        class="btn-search btn rounded-pill shadow"
      >Sign Up</button>
    </form>
  </div>
</section>
{{!-- reference javascript file --}}
<script src="/javascript/sign-up.js"></script>
